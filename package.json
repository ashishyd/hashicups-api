{
  "name": "yassir-platform-challenge",
  "version": "1.0.0",
  "description": "",
  "main": "main.js",
  "types": "main.ts",
  "private": true,
  "scripts": {
    "get": "cd packages/iac && cdktf get",
    "build": "tsc",
    "hashicups-start": "cd packages/iac/docker_compose && docker-compose up",
    "hashicups-stop": "cd packages/iac/docker_compose && docker-compose down",
    "synth": "cd packages/iac && cdktf synth",
    "deploy": "cd packages/iac && cdktf deploy --auto-approve",
    "compile": "tsc --pretty",
    "watch": "tsc -w",
    "test": "jest",
    "test:watch": "jest --watch",
    "upgrade": "npm i cdktf@latest cdktf-cli@latest",
    "upgrade:next": "npm i cdktf@next cdktf-cli@next",
    "setup": "ts-node ./packages/iac/initial-setup.ts",
    "watch-orders": "ts-node ./packages/iac/watch-orders.ts"
  },
  "engines": {
    "node": ">=16",
    "pnpm": ">=7"
  },
  "packageManager": "pnpm@v7.25.0",
  "devDependencies": {
    "@types/jest": "^29.4.0",
    "@types/node": "^18.13.0",
    "@typescript-eslint/eslint-plugin": "^5.48.2",
    "@typescript-eslint/parser": "^5.48.2",
    "dotenv": "^16.0.3",
    "eslint": "^8.32.0",
    "eslint-config-prettier": "^8.6.0",
    "jest": "^29.4.3",
    "prettier": "^2.8.3",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "cdktf": "^0.15.5",
    "constructs": "^10.1.265"
  }
